/* Range filter */
.text-range {
    color: #4D6077;
    font-size: 16px;
}
input[type=range] {
    -webkit-appearance: none;
    width: 100%;
    height: 10px;
    border-radius: 5px;
    background: linear-gradient(to right, #2787FB 0%, #e0e0e0 0%);
    outline: none;
    transition: background 0.3s ease;
    z-index: 1;
    position: relative;
}

input[type=range]::-webkit-slider-thumb {
    -webkit-appearance: none;
    appearance: none;
    width: 24px;
    height: 24px;
    border-radius: 50%;
    background: #2787FB;
    border: 2px solid #fff;
    cursor: pointer;
    z-index: 2;
    position: relative;
    margin-top: -7px;
}

input[type=range]::-moz-range-thumb {
    width: 24px;
    height: 24px;
    border-radius: 50%;
    background: #2787FB;
    border: 2px solid #fff;
    cursor: pointer;
    z-index: 2;
    position: relative;
}

.tooltip-custom {
    position: absolute;
    top: -85px;
    left: 0;
    background: white;
    padding: 8px 8px;
    border-radius: 6px;
    box-shadow: 0 2px 6px rgba(0,0,0,0.2);
    font-size: 14px;
    text-align: center;
    white-space: nowrap;
    transform: translateX(-42%);
    z-index: 3;
    display: flex;
    align-items:center;
    text-align: left;
}
.tooltip-custom strong {
    font-weight: bolder;
    background: #e1ebf7;
    font-size: 20px;
    padding: 12px 12px;
    margin-left: 10px;
    border-radius: 5px;
}
.tooltip-custom::after {
    content: "";
    position: absolute;
    bottom: -10px;
    left: 50%;
    transform: translateX(-50%);
    border-width: 6px;
    border-style: solid;
    border-color: white transparent transparent transparent;
}


