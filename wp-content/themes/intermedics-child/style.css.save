/****** CF7 **/

/* Input Fields */
.cf7-custom-input {
    background: transparent;
    margin-right: 60px;
    border: none;
    border-radius: 50px;
    padding: 22px 44px;
    color: #ffffff;
    box-shadow: inset 0 4px 6px rgba(0, 0, 0, 0.4),inset 0 -4px 6px rgba(39, 135, 251, 0.3);
    transition: all 0.3s ease;
    width: 100%;
    font-family: 'Roboto', sans-serif;
    font-weight: 400;
    font-size: 24px;
    max-width: 415px;
    text-align: left;
}

.cf7-custom-input::placeholder {
    color: #B0B8D9;
    opacity: 0.7;
}

.cf7-custom-input:focus {
    outline: none;
    box-shadow: inset 0 4px 6px rgba(0, 0, 0, 0.4),
        inset 0 -4px 6px rgba(39, 135, 251, 0.5),
        0 0 10px rgba(39, 135, 251, 0.6);
}

.cf7-custom-btn {
    background-color: #2787FB;
    border: none;
    border-radius: 50px;
    padding: 20px 90px;
    display: flex;
    align-items: center;
    gap: 8px;
    font-weight: 400;
    font-family: 'Oxygen', sans-serif;
    color: #fff;
    font-size: 24px;
    transition: all 0.3s ease;
    box-shadow: 0 4px 10px rgba(39, 135, 251, 0.4);
}

.cf7-custom-btn:hover {
    background-color: #1d6fd1;
    box-shadow: 0 4px 15px rgba(39, 135, 251, 0.6);
}
button.cf7-custom-btn.newsletter-btn {
    padding: 12px 42px;
}

